{
  "restartable": "rs",
  "ignore": [
    ".git",
    ".svn",
    "node_modules/**/node_modules"
  ],
  "verbose": true,
  "execMap": {
    "js": "node --harmony"
  },
  "watch": [
    "src/index.js",
    "src/**/",
    "public/static/**/",
    "public/**"
  ],
  "env": {
    "NODE_ENV": "development"
  },
  "ext": "ejs js json mustache css html"
}
